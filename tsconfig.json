{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "baseUrl": "app",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "dist",
    "jsx": "react",
    "allowJs": true,
    "allowUnreachableCode": true
  },
  "exclude": [
    "node_modules"
  ]
}
